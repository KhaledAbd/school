@model IEnumerable<int>
@{
    ViewData["Title"] = "Index";
}

<nav class="navbar navbar-expand-sm navbar-dark justify-content-center">
    <ul class="navbar-nav">
        <li class="nav-item list-inline-item">
            <a class="nav-link" href="#">
                <div class="leftIcon">
                    <i class="fas fa-home" title="الصفحةُ الرئيسية"></i>
                </div>
            </a>
        </li>
        <li class="nav-item list-inline-item">
            <a class="nav-link" href="#">
                <div class="rightIcon">
                    <i class="fas fa-university" title="السنواتُ الدراسيّة"></i>
                </div>
            </a>
        </li>
    </ul>
</nav>


<div class="row2">
    @foreach (var month in Model)
    {

    <a asp-action="Table" asp-controller="Absent"
       asp-route-month="@month" asp-route-year="2020" title="الشهر @month">الشهر @month </a>
    }
</div>

<link rel="stylesheet" href="~/css/styleClasses.css" />
<script src="~/js/custom-classes.js"></script>
